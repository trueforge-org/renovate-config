{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
      customDatasources: {
    qbittorrent: {
      defaultRegistryUrlTemplate: 'https://github.com/userdocs/qbittorrent-nox-static/releases/latest/download/dependency-version.json',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": qbittorrent}]}',
      ],
    },
    'servarr-develop': {
      defaultRegistryUrlTemplate: 'https://{{packageName}}.servarr.com/v1/update/develop/changes?os=linux&runtime=netcore',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": $[0].version}]}',
      ],
    },
    'servarr-plugins': {
      defaultRegistryUrlTemplate: 'https://{{packageName}}.servarr.com/v1/update/plugins/changes?os=linux&runtime=netcore',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": $[0].version}]}',
      ],
    },
    'servarr-nightly': {
      defaultRegistryUrlTemplate: 'https://{{packageName}}.servarr.com/v1/update/nightly/changes?os=linux&runtime=netcore',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": $[0].version}]}',
      ],
    },
    'servarr-eros': {
      defaultRegistryUrlTemplate: 'https://{{packageName}}.servarr.com/v1/update/eros/changes?os=linux&runtime=netcore',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": $[0].version}]}',
      ],
    },
    'sonarr-develop': {
      defaultRegistryUrlTemplate: 'https://services.sonarr.tv/v1/update/develop/changes?os=linux&runtime=netcore&version=4.0',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": $[0].version}]}',
      ],
    },
    plex: {
      defaultRegistryUrlTemplate: 'https://plex.tv/api/downloads/5.json',
      format: 'json',
      transformTemplates: [
        '{"releases":[{"version": computer.Linux.version}]}',
      ],
    },
  },
  customManagers: [
    {
      customType: 'regex',
      description: 'Process Annotations in Docker Bake',
      managerFilePatterns: [
        '/(^|/)docker-bake\\.hcl$/',
      ],
      matchStrings: [
        'datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)( versioning=(?<versioning>\\S+))?\\n.+ = "(?<currentValue>[^"]+)"',
      ],
      datasourceTemplate: '{{#if datasource}}{{{datasource}}}{{else}}github-releases{{/if}}',
      versioningTemplate: '{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}',
    },
    {
      customType: 'regex',
      description: 'Process Annotations in Dockerfiles',
      managerFilePatterns: [
        '/(^|/)Dockerfile$/',
      ],
      matchStrings: [
        'datasource=(?<datasource>\\S+) depName=(?<depName>\\S+)( versioning=(?<versioning>\\S+))?\\n.+(:\\s|=)(&\\S+\\s)?(?<currentValue>\\S+)',
      ],
      datasourceTemplate: '{{#if datasource}}{{{datasource}}}{{else}}github-releases{{/if}}',
      versioningTemplate: '{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}',
    },
  ],
  packageRules: [
    {
      matchDatasources: [
        'docker',
      ],
      digest: {
        enabled: true,
      },
    },
    {
      matchFileNames: [
        '**/.mise.toml',
      ],
      pinDigests: false,
    },
   {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/ubuntu',
      ],
      groupName: 'ubuntu',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'ubuntu-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/python',
      ],
      groupName: 'python',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'python-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/golang',
      ],
      groupName: 'golang',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'golang-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/node',
      ],
      groupName: 'node',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'node-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/python-node',
      ],
      groupName: 'python-node',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'python-node-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/java8',
      ],
      groupName: 'java8',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'java8-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/java11',
      ],
      groupName: 'java11',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'java11-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/java17',
      ],
      groupName: 'java17',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'java17-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/java21',
      ],
      groupName: 'java21',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'java21-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/java25',
      ],
      groupName: 'java25',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'java25-dep',
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'ghcr.io/trueforge-org/steamcmd',
      ],
      groupName: 'steamcmd',
      additionalBranchPrefix: 'update-',
      semanticCommitScope: 'steamcmd-dep',
    },
  ]
}
